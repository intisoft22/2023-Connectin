<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="report_sales_order_xls" model="ir.actions.report">
        <field name="name">Download Excel</field>
        <field name="model">sales.report</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">shopee_report.report_sales_order_xls</field>
        <field name="report_file">shopee_report.report_sales_order_xls</field>
        <field name="binding_model_id" ref="model_sales_report"/>
        <field name="binding_type">report</field>
        <field name="print_report_name">('SO - ') + object.month2name(int(object.month)) + (' ') + str(object.year) + (' Report')</field>
    </record>

</odoo>
